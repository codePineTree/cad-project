<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cad.cadproject.CADAdmin.CADAdminMapper">
<select id="getFilePathByModelId" resultType="string">
    SELECT FILE_PATH
    FROM CAD_MODEL
    WHERE MODEL_ID = #{modelId}
</select>



</mapper>
